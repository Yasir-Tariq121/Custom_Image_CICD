{
    "executionRoleArn": "",
    "containerDefinitions": [
        {
            "name": "nginx",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "networkMode": "bridge",
    "cpu": "512",
    "memory": "1024",
    "family": "task-nginx"
}
